package Excercise3;

import java.awt.Color;
import java.awt.Point;

public class Birch extends Tree {

	protected Point pos;
	protected double size;

	public Birch(Point p, double s) {
		this.pos = p;
		this.size = s;
	}
	
	public Color getColor() {
		return Color.green;
	}
	
	public double getSize() {
		return this.size;
	}

}
